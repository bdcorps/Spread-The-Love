{
"manifest_version": 2,
    "name": "Spread The Love",
    "description": "Replaces all the negative words with the positive ones to promote a clean web experience",
    "version": "2",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}